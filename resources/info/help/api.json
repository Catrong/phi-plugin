[
    {
        "group": "查分API公测介绍",
        "list": [
            {
                "title": "查分ID",
                "eg": "",
                "desc": "您的通用查分ID，可以在任意连接查分平台的BOT处使用指令进行绑定"
            },
            {
                "title": "API token",
                "eg": "",
                "desc": "您的API账户密码，设置后可以管理查分账户"
            },
            {
                "title": "权限管理",
                "eg": "",
                "desc": "通过id绑定后仅能获取与查询，评论等功能需要绑定sessionToken或验证API token"
            }
        ]
    },
    {
        "group": "绑定",
        "list": [
            {
                "title": "/bind",
                "eg": "/bind <userId>",
                "desc": "绑定你的 userId<br>eg1: /bind 123456"
            },
            {
                "title": "/unbind",
                "eg": "/unbind",
                "desc": "在 Bot 上删除储存的你的存档以及成绩记录并且解除云端账号对该平台的绑定"
            }
        ]
    },
    {
        "group": "管理",
        "list": [
            {
                "title": "/setApiToken",
                "eg": "/setApiToken <api Token>",
                "desc": "设置初始API Token（API密码），用于管理账户<br>eg: /setApiToken xxxxxxxx"
            },
            {
                "title": "/tkls",
                "eg": "/tkls",
                "desc": "获取当前绑定的平台列表"
            },
            {
                "title": "/tokenManage",
                "eg": "/tokenManage (delete|rmau) <平台序号> (-f)?",
                "desc": "删除绑定的平台，-f 跳过确认<br>eg: /tokenManage delete 1 -f"
            },
            {
                "title": "/auth",
                "eg": "/auth <api Token>",
                "desc": "验证API Token 获取管理账户权限"
            },
            {
                "title": "/clearApiData",
                "eg": "/clearApiData",
                "desc": "清除API数据"
            },
            {
                "title": "/noapi",
                "eg": "/noapi",
                "desc": "不参与数据统计，不影响正常使用API"
            }
        ]
    },
    {
        "group": "管理员命令",
        "auth": "master",
        "list": [
            {
                "title": "/updateUserToken",
                "eg": "/updateUserToken",
                "desc": "上传当前BOT端的用户 sessionToken 到API端"
            },
            {
                "title": "/updateComment",
                "eg": "/updateComment",
                "desc": "上传当前BOT端的用户评论到API端"
            }
        ]
    }
]